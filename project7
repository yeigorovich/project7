#include <stdio.h>
#include <math.h>

int main() {
    // Введення вхідних даних
    int x1, y1, r1, x2, y2, r2;
    printf("Введіть координати та радіус першого кола (x1, y1, r1): ");
    scanf("%d %d %d", &x1, &y1, &r1);
    printf("Введіть координати та радіус другого кола (x2, y2, r2): ");
    scanf("%d %d %d", &x2, &y2, &r2);

    // Визначення відстані між центрами колів
    double distance = sqrt(pow(x2 - x1, 2) + pow(y2 - y1, 2));

    // Перевірка на умову безкінечно багатьох точок перетину
    if (distance == 0 && r1 == r2) {
        printf("Кола повністю співпадають, точок перетину безліч.\n");
        return 0;
    }

    // Визначення кількості точок перетину
    if (distance > r1 + r2 || distance < fabs(r1 - r2)) {
        printf("Кола не перетинаються, точок перетину немає.\n");
    } else if (distance == r1 + r2 || distance == fabs(r1 - r2)) {
        printf("Кола торкаються одна одну, одна точка перетину.\n");
    } else {
        printf("Кола перетинаються, дві точки перетину.\n");
    }

    return 0;
}
